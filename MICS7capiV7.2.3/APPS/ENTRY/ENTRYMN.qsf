    conditions:
      - questionText:
          ENG: |
            <p class="MsoNormal" style="line-height:115%"><span style="font-size:10.0pt;
            line-height:115%;mso-ansi-language:EN-US"><span class="Qtxt">NEARBY LANDMARK (E.G. CHURCH, SCHOOL,
            CLINIC)</span><o:p></o:p></span></p><p></p>
